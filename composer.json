{
	"name": "ublaboo/api-docu",
	"type": "library",
	"description": "Documentation generating for ApiRouter routes - awesome runtime documentation",
	"keywords": ["rest", "api", "documentation", "docu", "apidocu", "routes", "nette"],
	"homepage": "https://ublaboo.org/api-docu",
	"license": ["MIT"],
	"support": {
		"issues": "https://github.com/ublaboo/api-docu/issues"
	},
	"authors": [
		{
			"name": "Pavel Janda",
			"homepage": "http://paveljanda.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Ublaboo\\ApiDocu\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Ublaboo\\ApiDocu\\Tests\\": "tests"
		}
	},
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/matlad/api-router.git"
        }
    ],
	"require": {
		"php": ">= 7.1.0",
		"nette/http": "^3.0",
		"nette/application": "^3.0",
		"nette/di": "^3.0",
		"latte/latte": "^2.5 || ^3.0",
		"ublaboo/api-router": "dev-nette-3.0"
	},
	"require-dev": {
		"nette/tester": "~1.6.1",
		"mockery/mockery": "~0.9",
		"tracy/tracy": "^2.3|^2.4"
	}
}
